package warmup;

public class User {
	
	private String name;
	private Calendar cal;
	
	public User(String name) {
		this.name = name;
	}
	
	public String getName() {
		return this.name;
	}
	
	private void createNewEvent() {
		Event event = new Event("lunch", new Date(), new Date());
		this.cal.addEvent(event)
	}

}
