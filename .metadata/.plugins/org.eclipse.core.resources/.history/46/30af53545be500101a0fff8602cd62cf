package warmupTests;

import static org.junit.Assert.*;

import java.util.Date;
import java.util.PriorityQueue;

import org.junit.Before;
import org.junit.Test;

import warmup.*;

public class CalendarTest {
	
	private Calendar cal;
	private User owner;
	
	@Before
	public void init() {
		owner = new User("Dani");
		cal = new Calendar(owner, "Home");
	}
	
	@Test
	public void shouldAddEvent() {
		PriorityQueue<Event> events = cal.getEvents();
		assertTrue(events.isEmpty());
		Date now = new Date();
		Event event = new Event("Dinner with mom", now, now);
		
	}

}
